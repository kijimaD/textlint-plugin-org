{
    "name": "textlint-plugin-org",
    "version": "0.2.2",
    "description": "Textlint plugin for Org mode",
    "repository": {
        "type": "git",
        "url": "git@github.com:kijimaD/textlint-plugin-org.git"
    },
    "author": "Kijima Daigo",
    "license": "GPLv3",
    "bugs": {
        "url": "https://github.com/kijimaD/textlint-plugin-org/issues"
    },
    "homepage": "https://github.com/kijimaD/textlint-plugin-org#readme",
    "main": "lib/index.js",
    "files": [
        "lib",
        "src"
    ],
    "directories": {
        "test": "test"
    },
    "scripts": {
        "build": "tsc -b && tsc -b tsconfig.json",
        "prepublish": "npm run --if-present build",
        "test": "mocha \"test/**/*.{js,ts}\"",
        "watch": "tsc -b --watch",
        "lint": "npx eslint src/**/*.ts"
    },
    "dependencies": {
        "@types/date-fns": "^2.6.0",
        "orga": "2.4.9",
        "structured-source": "^3.0.2"
    },
    "devDependencies": {
        "@textlint/ast-tester": "^12.0.0",
        "@textlint/kernel": "^12.0.0",
        "@types/jest": "^27.0.1",
        "@types/power-assert": "^1.5.3",
        "@types/traverse": "^0.6.32",
        "@typescript-eslint/eslint-plugin": "^5.2.0",
        "@typescript-eslint/parser": "^5.2.0",
        "eslint": "^8.1.0",
        "eslint-plugin-import": "^2.23.4",
        "mocha": "^9.0.0",
        "power-assert": "^1.6.1",
        "textlint-rule-max-comma": "^2.0.2",
        "traverse": "^0.6.6",
        "ts-node": "^10.0.0",
        "ts-node-test-register": "^10.0.0",
        "typescript": "^4.3.2"
    }
}
